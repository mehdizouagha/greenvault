/* Style for the QTabBar (the row of tabs) */
QTabBar::tab {
    background-color: #737373;  
    color: white;             
    border: 2px solid black;  
    border-radius: 10px;      
     margin-right: 10px; 
    padding: 10px 20px;         
    min-width: 100px;         
    font-size: 16px;            
}

/* Hover effect for tabs */
